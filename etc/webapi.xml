<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="GET" url="/V1/rafique-adminapi/getvalue">
		<service class="Rafique\AdminApi\Api\GetValueManagementInterface" method="getGetValue"/>
		<resources>
			<resource ref="Magento_Backend::store"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/rafique-adminapi/setvalue">
		<service class="Rafique\AdminApi\Api\SetValueManagementInterface" method="putSetValue"/>
		<resources>
			<resource ref="Magento_Backend::store"/>
		</resources>
	</route>
</routes>
